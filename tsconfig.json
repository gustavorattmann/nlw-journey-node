{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "21.0.0",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "ES2022",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "noEmit": true,
    "allowImportingTsExtensions": false
  }
}